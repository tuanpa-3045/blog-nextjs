{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "next", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "no-var": "error",
    "prettier/prettier": ["error"],
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
